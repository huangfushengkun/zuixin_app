// 不能用const wxs 与js是不同的语言
var format = function (text) {
    var reg = getRegExp('\\\\n','g')
    // 初始化时text为nudefind
    return text && text.replace(reg,'\n&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;')
}
var limit =  function (array,length) {
    return array.slice(0, length)
}
module.exports = {
    format:format,
    limit:limit
}